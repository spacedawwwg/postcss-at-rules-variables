{
  "name": "postcss-at-rules-variables-2",
  "version": "1.0.2",
  "description": "PostCSS plugin to use CSS Custom Properties in at-rule @each, @for, @if, @else ",
  "license": "MIT",
  "repository": "spacedawwwg/postcss-at-rules-variables-2",
  "homepage": "spacedawwwg/postcss-at-rules-variables-2#readme",
  "bugs": "spacedawwwg/postcss-at-rules-variables-2/issues",
  "main": "lib/index.js",
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.6.0"
  },
  "type": "module",
  "scripts": {
    "prepare": "pnpm run build",
    "build": "rimraf lib && babel src -d lib",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint src/*.js --fix",
    "pretest": "pnpm run lint",
    "publish": "clean-publish",
    "upgrade:dependencies": "pnpx npm-check-updates --root --dep dev,optional,peer,prod,packageManager --upgrade --packageManager pnpm"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "plugins",
    "for",
    "if",
    "else",
    "each",
    "at-rules",
    "rules",
    "property"
  ],
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/preset-env": "7.25.3",
    "@babel/register": "7.24.6",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-angular": "19.3.0",
    "@vitest/coverage-v8": "2.0.5",
    "babel-preset-minify": "0.5.2",
    "clean-publish": "5.0.0",
    "conventional-changelog-cli": "5.0.0",
    "eslint": "9.8.0",
    "eslint-config-xo": "0.45.0",
    "eslint-config-xo-space": "0.35.0",
    "eslint-plugin-ava": "15.0.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-html": "8.1.1",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "55.0.0",
    "lint-staged": "15.2.7",
    "postcss": "8.4.40",
    "rimraf": "6.0.1",
    "vite": "5.3.5",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "postcss": "8.4.40"
  }
}
